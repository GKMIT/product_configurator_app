!!! 5
html.no-js(lang='en')
  //
     BEGIN HEAD 
  head
    meta(charset='utf-8')
    title
      block title_top
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(content='width=device-width, initial-scale=1.0', name='viewport')
    block meta_tags
    //
       BEGIN GLOBAL MANDATORY STYLES 
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all', rel='stylesheet', type='text/css')
    link(href='/assets/plugins/font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    link(href='/assets/plugins/bootstrap/css/bootstrap.min.css', rel='stylesheet', type='text/css')
    link(rel='stylesheet', type='text/css', href='/assets/plugins/bootstrap-datepicker/css/datepicker.css')

    //
       END GLOBAL MANDATORY STYLES 
    //
       BEGIN THEME STYLES 
    link(href='/assets/css/style-metronic.css', rel='stylesheet', type='text/css')
    link(href='/assets/css/style.css', rel='stylesheet', type='text/css')
    link(href='/assets/css/style-responsive.css', rel='stylesheet', type='text/css')
    link(href='/assets/css/plugins.css', rel='stylesheet', type='text/css')
    link#style_color(href='/assets/css/themes/default.css', rel='stylesheet', type='text/css')
    link(href='/assets/css/custom.css', rel='stylesheet', type='text/css')
    link(href='/assets/css/theme.bootstrap.css', rel='stylesheet', type='text/css')
    //
       END THEME STYLES 
    //
       BEGIN PAGE LEVEL STYLES 
    link(href='/assets/plugins/gritter/css/jquery.gritter.css', rel='stylesheet', type='text/css')
    //
       END PAGE LEVEL STYLES 
    link(rel='shortcut icon', href='/assets/img/favicon.ico')
  //
     END HEAD 
  //
     BEGIN BODY 
  body.page-header-fixed
    //
       BEGIN HEADER 
    .header.navbar.navbar-fixed-top
      //
         BEGIN TOP NAVIGATION BAR 
      .header-inner
        //
           BEGIN LOGO 
        a.navbar-brand(href='/users/',style="color:#fff;width:50%")
          //img.img-responsive(src='/assets/img/logo.png', alt='logo')
          | &nbsp;Product Configurator Application
        //
           END LOGO 
        //
           BEGIN RESPONSIVE MENU TOGGLER 
        a.navbar-toggle(href='javascript:;', data-toggle='collapse', data-target='.navbar-collapse')
          img(src='/assets/img/menu-toggler.png', alt='')
        //
           END RESPONSIVE MENU TOGGLER 
        //
           BEGIN TOP NAVIGATION MENU 
        ul.nav.navbar-nav.pull-right
          //
             BEGIN NOTIFICATION DROPDOWN 
          //
             BEGIN USER LOGIN DROPDOWN 
          li.dropdown.user
            a.dropdown-toggle(href='#', data-toggle='dropdown', data-hover='dropdown', data-close-others='true')
              img(alt='', src='/assets/img/avatar_small.png')
              span.username
                | &nbsp;#{username}
              i.fa.fa-angle-down
            ul.dropdown-menu
              li
                a(href='extra_profile.html')
                  i.fa.fa-user
                  | My Profile
              li
                a(href='/logout')
                  i.fa.fa-key
                  | Log Out
          //
             END USER LOGIN DROPDOWN 
        //
           END TOP NAVIGATION MENU 
      //
         END TOP NAVIGATION BAR 
    //
       END HEADER 
    .clearfix
    //
       BEGIN CONTAINER 
    .page-container
      //
         BEGIN SIDEBAR 
      .page-sidebar-wrapper
        .page-sidebar.navbar-collapse.collapse
          //
             BEGIN SIDEBAR MENU 
          ul.page-sidebar-menu(data-auto-scroll='true', data-slide-speed='200')
            li.sidebar-toggler-wrapper
              //
                 BEGIN SIDEBAR TOGGLER BUTTON 
              .sidebar-toggler.hidden-phone
              //
                 BEGIN SIDEBAR TOGGLER BUTTON 
            li.sidebar-search-wrapper
              br
            li(class=dashboard+' start')
              a(href='/users/')
                i.fa.fa-home
                span.title
                  | Dashboard
            li(class=rfq)
              a(href='javascript:;')
                i.fa.fa-shopping-cart
                span.title
                  | RFQ
                span.arrow
              ul.sub-menu
                li(class=sub_sidebar1)
                  a(href='/users/newrfq')
                    | New
                li(class=sub_sidebar2)
                  a(href='/users/finalize')
                    | Finalize
            li(class=bid)
              a(href='/users/bids')
                i.fa.fa-gift
                span.title
                  | Bid / No Bid
            li(class=tender_quote)
              a(href='/users/tendering_quote')
                i.fa.fa-gift
                span.title
                  | Tendering Quote
            li(class=quote)
              a(href='javascript:;')
                i.fa.fa-cogs
                span.title
                  | Quote
                span.arrow
              ul.sub-menu
                li(class=sub_sidebar1)
                  a(href='/users/quote_finalize')
                    | Finalize
                li(class=sub_sidebar2)
                  a(href='/users/follow_up')
                    | Follow Up
          //
             END SIDEBAR MENU 
      //
         END SIDEBAR 
      //
         BEGIN CONTENT 
      .page-content-wrapper
        .page-content
          //
             BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM
          #portlet-config.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel', aria-hidden='true')
            .modal-dialog
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal', aria-hidden='true')
                  h4.modal-title Modal title
                  | Widget settings form goes here
                .modal-footer
                  button.btn.blue(type='button') Save changes
                  button.btn.default(type='button', data-dismiss='modal') Close
              //
                 /.modal-content 
            //
               /.modal-dialog 
          //
             /.modal 
          //
             END SAMPLE PORTLET CONFIGURATION MODAL FORM
          //
             BEGIN PAGE HEADER
          .row
            .col-md-12
              //
                 BEGIN PAGE TITLE & BREADCRUMB
              h3.page-title
                block title
                small
                  block hint
              //
                 END PAGE TITLE & BREADCRUMB
          //
             END PAGE HEADER
          //
             BEGIN PAGE CONTENT
          block page_content
        //
           END PAGE CONTENT
      //
         BEGIN CONTENT 
    //
       END CONTAINER 
    //
       BEGIN FOOTER 
    .footer
      .footer-inner
        | 2014 Â© Metronic by keenthemes.
      .footer-tools
        span.go-top
          i.fa.fa-angle-up
    //
       END FOOTER 
    //
       BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) 
    //
       BEGIN CORE PLUGINS 
    //if lt IE 9
      script(src='/assets/plugins/respond.min.js')
      script(src='/assets/plugins/excanvas.min.js')
    script(src='/assets/plugins/jquery-1.10.2.min.js', type='text/javascript')
    script(src='/assets/plugins/jquery-migrate-1.2.1.min.js', type='text/javascript')
    script(src='/assets/plugins/bootstrap/js/bootstrap.min.js', type='text/javascript')
    script(src='/assets/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js', type='text/javascript')
    script(src='/assets/plugins/jquery.blockui.min.js', type='text/javascript')
    script(src='/assets/plugins/jquery.cokie.min.js', type='text/javascript')
    //
       END CORE PLUGINS 
    //
       BEGIN PAGE LEVEL PLUGINS 
    script(type='text/javascript', src='/assets/plugins/gritter/js/jquery.gritter.js')
    script(type='text/javascript', src='/assets/plugins/holder.js')
    script(type='text/javascript', src='/assets/scripts/custom/users.js')
    script(type='text/javascript', src='/assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js')
    script(type='text/javascript', src='/assets/scripts/custom/bootbox.min.js')
    script(type='text/javascript', src='/assets/scripts/custom/jquery.tablesorter.js')
    script(type='text/javascript', src='/assets/scripts/custom/jquery.tablesorter.pager.js')
    script(type='text/javascript', src='/assets/scripts/custom/jquery.tablesorter.widgets.js')
    //
       END PAGE LEVEL PLUGINS 
    //
       BEGIN PAGE LEVEL SCRIPTS 
    script(src='/assets/scripts/core/app.js')
    script(src='/assets/scripts/custom/ui-general.js')
    script(src='/assets/scripts/custom/components-pickers.js')
    //
       END PAGE LEVEL SCRIPTS
    block doc_ready
    script.
      jQuery(document).ready(function() {
      // initiate layout and plugins
        App.init();
        initialize();
      });
    //
       END JAVASCRIPTS 
  //
     END BODY 